pipeline {
  agent any
  tools {
    maven 'MAVEN' 
  }
  stages {
    stage ('Build') {
      steps {
        sh 'mvn clean package'
      }
      }
    stage ('Test') {
      steps {
        echo 'testing'
      }
    
    }
    stage ('publish') {
      steps {
        echo 'publishing....!'
      }
    
    }
    stage ('deploy') {
      steps {
        echo 'deploying....!'
      }
    
    }
  }
}
